int TOH(int n,char Sour, char Aux,char Des)
{ 
	if(n==0)
	{
        return 0
	}
	
	TOH(n-1,Sour,Des,Aux)
    print("Move Disk ") 
    print(n)
    print(" from ")
    print(Sour)
    print(" to ")
    print(Des)
    print("\n")
	TOH(n-1,Aux,Sour,Des)
}
void main()
{ 
	print("Enter no. of disks: ")	
	int n = to_int(input())

	TOH(n,'A','B','C')
}